<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>
    <iisnode>
      <node_env>development</node_env>
      <debuggingEnabled>true</debuggingEnabled>
      <loggingEnabled>true</loggingEnabled>
      <devErrorsEnabled>true</devErrorsEnabled>
    </iisnode>
  </system.webServer>
</configuration>